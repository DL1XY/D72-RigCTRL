                    ImageLeftWidget:
                        source: "assets/satellites/480px-ISS_emblem.png"

                ThreeLineAvatarListItem:
                    text: "AO-73 (FUNCUBE 1)"
                    secondary_text: "AOR: 2023-04-3 17:45:19"
                    tertiary_text: "AOR: 2023-04-3 17:45:19"

                    ImageLeftWidget:
                        source: "assets/satellites/funcube-1-ao-73-39444.jpeg"

            